generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_DATABASE_URL")
}

model LabbaikBot {
  id        Int      @id @default(autoincrement())
  version   Int      @default(1)
  createdAt DateTime @default(now())
}

model AttendenceThread {
  id String @id @unique @default(uuid()) @db.Uuid
}